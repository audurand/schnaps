<FunctionReward>
  <LocalVariables>
	  <LocalVariable label="Recompense">
		  <Double value="0"/>
	  </LocalVariable>
	  <LocalVariable label="Nb_Fractures_Evitees">
		  <Int value="0"/>
	  </LocalVariable>
  </LocalVariables>
  <PrimitiveTree>
    <Control_Sequential>
      <Data_SetVariable inValue="@Cout" outVariable="%Recompense"/>
      <Operators_MultVariable inArgLeft="%Recompense" inArgRight="-1" inArgRight_Type="Double" outResult="%Recompense"/>
      <Data_SetVariableComplex outVariable="%Nb_Fractures_Evitees">
		<Operators_SubComplex>
	      <Data_Value inValue="2" inValue_Type="Int"/>
	      <Operators_AddComplex>
		    <Operators_AddComplex>
		      <Data_Value inValue="@Nb_Fracture_Hanche"/>
		      <Data_Value inValue="@Nb_Fracture_Poignet"/>
		    </Operators_AddComplex>
		      <Data_Value inValue="@Nb_Fracture_Colonne"/>
	      </Operators_AddComplex>
	    </Operators_SubComplex>
      </Data_SetVariableComplex>
      <Control_IfThenElse>
	    <Operators_IsGreater inArgLeft="%Nb_Fractures_Evitees" inArgRight="0" inArgRight_Type="Int"/>
	    <Operators_DivVariable inArgLeft="%Recompense" inArgRight="%Nb_Fractures_Evitees" outResult="%Recompense"/>
	    <Control_Nothing/>
      </Control_IfThenElse>
      <Data_Value inValue="%Recompense"/>
    </Control_Sequential>
  </PrimitiveTree>
</FunctionReward>
