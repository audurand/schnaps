  <Process label="Post-Detection">
    <LocalVariables/>
    <PrimitiveTree size="1">
      <Control_IfThenElse>
        <Data_Value inValue="@Mort"/>
        <Control_Nothing/>
        <Control_IfThenElse>
          <Operators_AndComplex>
            <!--L'individu est aveugle des deux yeux ou a deja ete traite pour tout-->
            <Operators_OrComplex>
              <Operators_IsEqual inArgRight_Type="String" inArgRight="Aveugle" inArgLeft="@BDR_D"/>
              <Operators_IsEqual inArgRight_Type="String" inArgRight="Aveugle" inArgLeft="@DMO_D"/>
              <Operators_AndComplex>
                <Operators_IsEqual inArgRight_Type="Bool" inArgRight="true" inArgLeft="@Traitement_PDR_D"/>
                <Operators_IsEqual inArgRight_Type="Bool" inArgRight="true" inArgLeft="@Traitement_CSMO_D"/>
              </Operators_AndComplex>
            </Operators_OrComplex>
            <Operators_OrComplex>
              <Operators_IsEqual inArgRight_Type="String" inArgRight="Aveugle" inArgLeft="@BDR_G"/>
              <Operators_IsEqual inArgRight_Type="String" inArgRight="Aveugle" inArgLeft="@DMO_G"/>
              <Operators_AndComplex>
                <Operators_IsEqual inArgRight_Type="Bool" inArgRight="true" inArgLeft="@Traitement_PDR_G"/>
                <Operators_IsEqual inArgRight_Type="Bool" inArgRight="true" inArgLeft="@Traitement_CSMO_G"/>
              </Operators_AndComplex>
            </Operators_OrComplex>
          </Operators_AndComplex>
          <Control_Nothing/>
          <Control_Branch inProbability="$GoldStandard_Compliance">
            <Control_Sequential>
              <Control_ProcessCall inLabel="Ophtalmologiste"/>
              <Control_IfThenElse>
                <!--Traitement PDR oeil droit?-->
                <Data_Value inValue="@Resultat_GoldStandard_PDR_D"/>
                <Control_IfThenElse>
                  <Data_Value inValue="@Traitement_PDR_D"/>
                  <Control_Nothing/>
                  <Control_Sequential>
                    <Data_SetVariable inValue_Type="Bool" inValue="true" outVariable="@Traitement_PDR_D"/>
                    <Control_IfThenElse>
                      <Control_ProcessCall inLabel="Traitement_PDR"/>
                      <Data_SetVariable inValue_Type="String" inValue="Traite" outVariable="@BDR_D"/>
                      <Control_Nothing/>
                    </Control_IfThenElse>
                  </Control_Sequential>
                </Control_IfThenElse>
                <Control_Nothing/>
              </Control_IfThenElse>
              <Control_IfThenElse>
                <!--Traitement PDR oeil gauche?-->
                <Data_Value inValue="@Resultat_GoldStandard_PDR_G"/>
                <Control_IfThenElse>
                  <Data_Value inValue="@Traitement_PDR_G"/>
                  <Control_Nothing/>
                  <Control_Sequential>
                    <Data_SetVariable inValue_Type="Bool" inValue="true" outVariable="@Traitement_PDR_G"/>
                    <Control_IfThenElse>
                      <Control_ProcessCall inLabel="Traitement_PDR"/>
                      <Data_SetVariable inValue_Type="String" inValue="Traite" outVariable="@BDR_G"/>
                      <Control_Nothing/>
                    </Control_IfThenElse>
                  </Control_Sequential>
                </Control_IfThenElse>
                <Control_Nothing/>
              </Control_IfThenElse>
              <Control_IfThenElse>
                <!--Traitement CSMO oeil droit?-->
                <Data_Value inValue="@Resultat_GoldStandard_CSMO_D"/>
                <Control_IfThenElse>
                  <Data_Value inValue="@Traitement_CSMO_D"/>
                  <Control_Nothing/>
                  <Control_Sequential>
                    <Data_SetVariable inValue_Type="Bool" inValue="true" outVariable="@Traitement_CSMO_D"/>
                    <Control_IfThenElse>
                      <Control_ProcessCall inLabel="Traitement_CSMO"/>
                      <Data_SetVariable inValue_Type="String" inValue="Traite" outVariable="@DMO_D"/>
                      <Control_Nothing/>
                    </Control_IfThenElse>
                  </Control_Sequential>
                </Control_IfThenElse>
                <Control_Nothing/>
              </Control_IfThenElse>
              <Control_IfThenElse>
                <!--Traitement CSMO oeil gauche?-->
                <Data_Value inValue="@Resultat_GoldStandard_CSMO_G"/>
                <Control_IfThenElse>
                  <Data_Value inValue="@Traitement_CSMO_G"/>
                  <Control_Nothing/>
                  <Control_Sequential>
                    <Data_SetVariable inValue_Type="Bool" inValue="true" outVariable="@Traitement_CSMO_G"/>
                    <Control_IfThenElse>
                      <Control_ProcessCall inLabel="Traitement_CSMO"/>
                      <Data_SetVariable inValue_Type="String" inValue="Traite" outVariable="@DMO_G"/>
                      <Control_Nothing/>
                    </Control_IfThenElse>
                  </Control_Sequential>
                </Control_IfThenElse>
                <Control_Nothing/>
              </Control_IfThenElse>
              <Control_ProcessPush inTarget="current" inDelay="6" inLabel="Post-Detection"/>
            </Control_Sequential>
            <Control_ProcessPush inTarget="current" inDelay="6" inLabel="Post-Detection"/>
          </Control_Branch>
        </Control_IfThenElse>
      </Control_IfThenElse>
    </PrimitiveTree>
  </Process>
