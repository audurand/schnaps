  <Process label="Optometriste">
    <LocalVariables/>
    <PrimitiveTree size="1">
      <Control_Sequential>
        <Control_IfThenElse>
          <!--Compter le cout et le passage au depistage seulement quand le systeme est stable.-->
          <Operators_IsGreaterComplex>
            <Data_Clock/>
            <Data_Value inValue_Type="ULong" inValue="11"/>
          </Operators_IsGreaterComplex>
          <Control_Sequential>
            <Operators_AddVariable inArgRight="@Optometriste_Cout" inArgLeft="@Cout" outResult="@Cout"/>
            <Control_ProcessPush inTarget="environment" inDelay="0" inLabel="Ajouter_Optometriste"/>
          </Control_Sequential>
          <Control_Nothing/>
        </Control_IfThenElse>
        <Operators_OrComplex>
          <Control_IfThenElse>
             <!--Detection BDR oeil droit-->
						 <Operators_IsEqual inArgRight_Type="String" inArgRight="PDR" inArgLeft="@BDR_D"/>
						 <Control_Branch inProbability="@Optometriste_Sensibilite">
              <Data_Value inValue_Type="Bool" inValue="true"/>
              <Data_Value inValue_Type="Bool" inValue="false"/>
						 </Control_Branch>
						 <Control_Branch inProbability="@Optometriste_Specificite">
              <Data_Value inValue_Type="Bool" inValue="false"/>
              <Data_Value inValue_Type="Bool" inValue="true"/>
						 </Control_Branch>
					</Control_IfThenElse>
          <Control_IfThenElse>
            <!--Detection BDR oeil gauche-->
						 <Operators_IsEqual inArgRight_Type="String" inArgRight="PDR" inArgLeft="@BDR_G"/>
						 <Control_Branch inProbability="@Optometriste_Sensibilite">
              <Data_Value inValue_Type="Bool" inValue="true"/>
              <Data_Value inValue_Type="Bool" inValue="false"/>
						 </Control_Branch>
						 <Control_Branch inProbability="@Optometriste_Specificite">
              <Data_Value inValue_Type="Bool" inValue="false"/>
              <Data_Value inValue_Type="Bool" inValue="true"/>
						 </Control_Branch>
					</Control_IfThenElse>
          <Control_IfThenElse>
            <!--Detection DMO oeil droit-->
						 <Operators_IsEqual inArgRight_Type="String" inArgRight="CSMO" inArgLeft="@DMO_D"/>
						 <Control_Branch inProbability="@Optometriste_Sensibilite">
              <Data_Value inValue_Type="Bool" inValue="true"/>
              <Data_Value inValue_Type="Bool" inValue="false"/>
						 </Control_Branch>
						 <Control_Branch inProbability="@Optometriste_Specificite">
              <Data_Value inValue_Type="Bool" inValue="false"/>
              <Data_Value inValue_Type="Bool" inValue="true"/>
						 </Control_Branch>
					</Control_IfThenElse>
          <Control_IfThenElse>
            <!--Detection DMO oeil gauche-->
						 <Operators_IsEqual inArgRight_Type="String" inArgRight="CSMO" inArgLeft="@DMO_G"/>
						 <Control_Branch inProbability="@Optometriste_Sensibilite">
              <Data_Value inValue_Type="Bool" inValue="true"/>
              <Data_Value inValue_Type="Bool" inValue="false"/>
						 </Control_Branch>
						 <Control_Branch inProbability="@Optometriste_Specificite">
              <Data_Value inValue_Type="Bool" inValue="false"/>
              <Data_Value inValue_Type="Bool" inValue="true"/>
						 </Control_Branch>
					</Control_IfThenElse>
        </Operators_OrComplex>
      </Control_Sequential>
    </PrimitiveTree>
  </Process>
