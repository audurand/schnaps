  <Process label="Optometriste">
    <LocalVariables/>
    <PrimitiveTree size="1">
      <Control_Sequential>
        <Control_IfThenElse>
          <!--Compter le cout et le passage au depistage seulement quand le systeme est stable.-->
          <Operators_IsGreaterComplex>
            <Data_Clock/>
            <Data_Value inValue_Type="ULong" inValue="11"/>
          </Operators_IsGreaterComplex>
          <Control_Sequential>
            <Operators_AddVariable inArgRight="@Optometriste_Cout" inArgLeft="@Cout" outResult="@Cout"/>
            <Control_ProcessPush inTarget="environment" inUnits="month" inDelay="0" inLabel="Ajouter_Optometriste"/>
          </Control_Sequential>
          <Control_Nothing/>
        </Control_IfThenElse>
        <Operators_OrComplex>
			<Control_ChoiceIsEqual inChoices="$ref.BDR" inValue="@BDR">
				<!--Detection BDR/PDR un oeil-->
				<Control_Branch inProbability="@Optometriste_Specificite">
				  <Data_Value inValue_Type="Bool" inValue="false"/>
				  <Data_Value inValue_Type="Bool" inValue="true"/>
				</Control_Branch>
				<Control_Branch inProbability="@Optometriste_Sensibilite">
				  <Data_Value inValue_Type="Bool" inValue="true"/>
				  <Data_Value inValue_Type="Bool" inValue="false"/>
				</Control_Branch>
				<Control_Branch inProbability="@Optometriste_Sensibilite">
				  <Data_Value inValue_Type="Bool" inValue="true"/>
				  <Data_Value inValue_Type="Bool" inValue="false"/>
				</Control_Branch>
				<Control_Branch inProbability="@Optometriste_Sensibilite">
				  <Data_Value inValue_Type="Bool" inValue="true"/>
				  <Data_Value inValue_Type="Bool" inValue="false"/>
				</Control_Branch>
				<Data_Value inValue_Type="Bool" inValue="false"/>
				<Data_Value inValue_Type="Bool" inValue="false"/>
			</Control_ChoiceIsEqual>
			<Control_IfThenElse>
				<Data_Value inValue="@Deux_Yeux_BDR"/>
				<Control_ChoiceIsEqual inChoices="$ref.BDR" inValue="@BDR">
					<!--Detection BDR/PDR l'autre oeil-->
					<Control_Branch inProbability="@Optometriste_Specificite">
					  <Data_Value inValue_Type="Bool" inValue="false"/>
					  <Data_Value inValue_Type="Bool" inValue="true"/>
					</Control_Branch>
					<Control_Branch inProbability="@Optometriste_Sensibilite">
					  <Data_Value inValue_Type="Bool" inValue="true"/>
					  <Data_Value inValue_Type="Bool" inValue="false"/>
					</Control_Branch>
					<Control_Branch inProbability="@Optometriste_Sensibilite">
					  <Data_Value inValue_Type="Bool" inValue="true"/>
					  <Data_Value inValue_Type="Bool" inValue="false"/>
					</Control_Branch>
					<Control_Branch inProbability="@Optometriste_Sensibilite">
					  <Data_Value inValue_Type="Bool" inValue="true"/>
					  <Data_Value inValue_Type="Bool" inValue="false"/>
					</Control_Branch>
					<Data_Value inValue_Type="Bool" inValue="false"/>
					<Data_Value inValue_Type="Bool" inValue="false"/>
				</Control_ChoiceIsEqual>
				<Data_Value inValue_Type="Bool" inValue="false"/>
			</Control_IfThenElse>
			<Control_ChoiceIsEqual inChoices="$ref.DMO" inValue="@DMO">
				<!--Detection DMO/CSMO un oeil-->
				<Control_Branch inProbability="@Optometriste_Specificite">
				  <Data_Value inValue_Type="Bool" inValue="false"/>
				  <Data_Value inValue_Type="Bool" inValue="true"/>
				</Control_Branch>
				<Control_Branch inProbability="@Optometriste_Sensibilite">
				  <Data_Value inValue_Type="Bool" inValue="true"/>
				  <Data_Value inValue_Type="Bool" inValue="false"/>
				</Control_Branch>
				<Control_Branch inProbability="@Optometriste_Sensibilite">
				  <Data_Value inValue_Type="Bool" inValue="true"/>
				  <Data_Value inValue_Type="Bool" inValue="false"/>
				</Control_Branch>
				<Data_Value inValue_Type="Bool" inValue="false"/>
				<Data_Value inValue_Type="Bool" inValue="false"/>
			</Control_ChoiceIsEqual>
			<Control_IfThenElse>
				<Data_Value inValue="@Deux_Yeux_DMO"/>
				<Control_ChoiceIsEqual inChoices="$ref.DMO" inValue="@DMO">
					<!--Detection DMO/CSMO l'autre oeil-->
					<Control_Branch inProbability="@Optometriste_Specificite">
					  <Data_Value inValue_Type="Bool" inValue="false"/>
					  <Data_Value inValue_Type="Bool" inValue="true"/>
					</Control_Branch>
					<Control_Branch inProbability="@Optometriste_Sensibilite">
					  <Data_Value inValue_Type="Bool" inValue="true"/>
					  <Data_Value inValue_Type="Bool" inValue="false"/>
					</Control_Branch>
					<Control_Branch inProbability="@Optometriste_Sensibilite">
					  <Data_Value inValue_Type="Bool" inValue="true"/>
					  <Data_Value inValue_Type="Bool" inValue="false"/>
					</Control_Branch>
					<Data_Value inValue_Type="Bool" inValue="false"/>
					<Data_Value inValue_Type="Bool" inValue="false"/>
				</Control_ChoiceIsEqual>
				<Data_Value inValue_Type="Bool" inValue="false"/>
			</Control_IfThenElse>
        </Operators_OrComplex>
      </Control_Sequential>
    </PrimitiveTree>
  </Process>
