<?xml version="1.0" encoding="UTF-8"?>
  <Process label="Assigner_Politique_MinCout">
    <LocalVariables/>
    <PrimitiveTree size="1">
      <Control_IfThenElse>
        <Operators_OrComplex>
          <Data_Value inValue="@Depistage"/>
          <Data_Value inValue="@Traitement"/>
        </Operators_OrComplex>
        <Control_Nothing/>
        <Control_IfThenElse>
          <Operators_IsEqualComplex>
            <Operators_ModComplex>
              <Data_Value inValue="@Age"/>
              <Data_Value inValue="10" inValue_Type="Int"/>
            </Operators_ModComplex>
            <Data_Value inValue="0" inValue_Type="Int"/>
          </Operators_IsEqualComplex>
          <Control_Sequential>
            <Control_IfThenElse>
              <Operators_IsEqual inArgRight="100" inArgRight_Type="Int" inArgLeft="@Age"/>
              <Control_Sequential>
                <!--Changement de politique: OST + prevention activite physique-->
                <Data_SetVariable inValue="false" outVariable="@Prevention" inValue_Type="Bool"/>
                <Data_SetVariable inValue="ActPhys" outVariable="@Methode_Prevention" inValue_Type="String"/>
                <Control_IfThenElse>
                  <Data_Value inValue="@ActPhys_Pratiquant"/>
                  <Data_SetVariable inValue="true" outVariable="@Pratiquant" inValue_Type="Bool"/>
                  <Data_SetVariable inValue="false" outVariable="@Pratiquant" inValue_Type="Bool"/>
                </Control_IfThenElse>
                <Control_Branch inProbability="$ref.compliance_depistage">
                  <Control_Sequential>
                    <Meds_Treatment inDiscountRate="$ref.taux_actualisation" inLabel="Visite_Medicale" inCompliance="1" outCost="@Cout" inChargeNonCompliant="false" inCost="102.78">
                      <Control_Nothing/>
                      <Control_Nothing/>
                    </Meds_Treatment>
                    <Control_ProcessCall inLabel="OST"/>
                  </Control_Sequential>
                  <Control_Nothing/>
                </Control_Branch>
              </Control_Sequential>
              <Control_Nothing/>
            </Control_IfThenElse>
            <Control_ProcessPush inDelay="10" inTarget="current" inLabel="Assigner_Politique_MinCout"/>
          </Control_Sequential>
          <Control_ProcessPush inDelay="1" inTarget="current" inLabel="Assigner_Politique_MinCout"/>
        </Control_IfThenElse>
      </Control_IfThenElse>
    </PrimitiveTree>
  </Process>
