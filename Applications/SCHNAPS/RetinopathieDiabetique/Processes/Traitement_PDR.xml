  <Process label="Traitement_PDR">
    <LocalVariables/>
    <PrimitiveTree size="1">
      <Control_Sequential>
        <Control_IfThenElse>
          <!--Comptabiliser les couts seulement quand le systeme est stable.-->
          <Operators_IsGreaterComplex>
            <Data_Clock/>
            <Data_Value inValue_Type="ULong" inValue="11"/>
          </Operators_IsGreaterComplex>
          <Control_Sequential>
            <Operators_AddVariable inArgLeft="@Cout" inArgRight="$ref.Traitement_PDR_Cout" inArgRight_Type="Double" outResult="@Cout"/>
            <Control_ProcessPush inTarget="environment" inDelay="0" inLabel="Ajouter_Ophtalmologiste"/>
          </Control_Sequential>
          <Control_Nothing/>
        </Control_IfThenElse>
        <Control_Branch inProbability="$ref.Traitement_PDR_Efficacite" probability="">
          <Data_Value inValue_Type="Bool" inValue="true"/>
          <Control_Sequential>
            <Control_IfThenElse>
              <!--Comptabiliser les couts seulement quand le systeme est stable.-->
              <Operators_IsGreaterComplex>
                <Data_Clock/>
                <Data_Value inValue_Type="ULong" inValue="11"/>
              </Operators_IsGreaterComplex>
              <Control_Sequential>
                <Operators_AddVariable inArgLeft="@Cout" inArgRight="$ref.Traitement_PDR_Cout" inArgRight_Type="Double" outResult="@Cout"/>
                <Control_ProcessPush inTarget="environment" inDelay="0" inLabel="Ajouter_Ophtalmologiste"/>
              </Control_Sequential>
              <Control_Nothing/>
            </Control_IfThenElse>
            <Control_Branch inProbability="$ref.Traitement_PDR_Efficacite">
              <Data_Value inValue_Type="Bool" inValue="true"/>
              <Control_Sequential>
                <Control_IfThenElse>
                  <!--Comptabiliser les couts seulement quand le systeme est stable.-->
                  <Operators_IsGreaterComplex>
                    <Data_Clock/>
                    <Data_Value inValue_Type="ULong" inValue="11"/>
                  </Operators_IsGreaterComplex>
                  <Control_Sequential>
                    <Operators_AddVariable inArgLeft="@Cout" inArgRight="$ref.Traitement_PDR_Cout" inArgRight_Type="Double" outResult="@Cout"/>
                    <Control_ProcessPush inTarget="environment" inDelay="0" inLabel="Ajouter_Ophtalmologiste"/>
                  </Control_Sequential>
                  <Control_Nothing/>
                </Control_IfThenElse>
                <Control_Branch inProbability="$ref.Traitement_PDR_Efficacite">
                  <Data_Value inValue_Type="Bool" inValue="true"/>
                  <Data_Value inValue_Type="Bool" inValue="false"/>
                </Control_Branch>
              </Control_Sequential>
            </Control_Branch>
          </Control_Sequential>
        </Control_Branch>
      </Control_Sequential>
    </PrimitiveTree>
  </Process>
