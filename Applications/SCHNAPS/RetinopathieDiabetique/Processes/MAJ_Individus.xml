 <Process label="MAJ_Individus">
  <LocalVariables/>
  <PrimitiveTree size="1">
   <Control_IfThenElse>
    <Data_Value inValue="@Mort"/>
    <Control_Nothing/>
    <Control_IfThenElse>
     <Operators_IsEqualComplex>
      <Operators_ModComplex>
       <Data_Clock/>
       <Data_Value inValue_Type="ULong" inValue="12"/>
      </Operators_ModComplex>
      <Data_Value inValue_Type="ULong" inValue="0"/>
     </Operators_IsEqualComplex>
     <Control_Sequential>
      <Control_IfThenElse>
       <Operators_OrComplex>
        <!--L'individu est aveugle d'un oeil-->
        <Operators_IsEqual inArgRight_Type="String" inArgRight="Aveugle" inArgLeft="@BDR_D"/>
        <Operators_IsEqual inArgRight_Type="String" inArgRight="Aveugle" inArgLeft="@DMO_D"/>
        <Operators_IsEqual inArgRight_Type="String" inArgRight="Aveugle" inArgLeft="@BDR_G"/>
        <Operators_IsEqual inArgRight_Type="String" inArgRight="Aveugle" inArgLeft="@DMO_G"/>
       </Operators_OrComplex>
       <Control_IfThenElse>
          <!--Compter les annees aveugles seulement quand le systeme est stable.-->
          <Operators_IsGreaterComplex>
            <Data_Clock/>
            <Data_Value inValue_Type="ULong" inValue="11"/>
          </Operators_IsGreaterComplex>
		  <Operators_AddVariable inArgRight_Type="Int" inArgRight="1" inArgLeft="@Annees_Aveugle" outResult="@Annees_Aveugle"/>
		  <Control_Nothing/>
       </Control_IfThenElse>
       <Control_Nothing/>
      </Control_IfThenElse>
      <Meds_Event inProbability="@Taux_Mortalite" inLabel="Deces">
       <Data_SetVariable inValue_Type="Bool" inValue="true" outVariable="@Mort"/>
       <Control_Sequential>
        <Control_ProcessCall inLabel="MAJ_Age"/>
        <Control_ProcessCall inLabel="MAJ_NIDDM"/>
        <Control_ProcessCall inLabel="MAJ_Taux_Mortalite"/>
       </Control_Sequential>
      </Meds_Event>
     </Control_Sequential>
     <Control_Nothing/>
    </Control_IfThenElse>
   </Control_IfThenElse>
  </PrimitiveTree>
 </Process>
