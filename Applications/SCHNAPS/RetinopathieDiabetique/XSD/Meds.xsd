						<pmt:attributeType>Double</pmt:attributeType>
					</xsd:appinfo>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="Meds_Test" substitutionGroup="_primitive">
