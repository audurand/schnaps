  <Process label="Evenements_AucunePrevention">
	<LocalVariables/>
    <PrimitiveTree size="1">
      <Control_IfThenElse>
        <Data_Value inValue="@Mort"/>
        <Control_Nothing/>
        <Control_Sequential>
          <Control_IfThenElse>
            <!--Appliquer les effets du traitement ou de la prevention s'il y a lieu.-->
            <Data_Value inValue="@Traitement"/>
            <Control_ProcessCall inLabel="Traitement_VitDCalcium"/>
            <Control_IfThenElse>
              <!--Appliquer prevention si individu a adpote prevention suite a une fracture-->
              <Data_Value inValue="@Prevention"/>
              <Control_ProcessCall inLabel="VitDCalcium"/>
              <Control_Nothing/>
            </Control_IfThenElse>
          </Control_IfThenElse>
          <Control_IfThenElse>
            <Operators_IsEqual inArgLeft="@Nb_Fracture_Hanche" inArgRight="2" inArgRight_Type="Int" />
            <Control_Nothing/>
            <Meds_Event inProbability="@Probabilite_Fracture_Hanche" inLabel="Fracture_Hanche">
              <Control_Sequential>
                <Control_ProcessCall inLabel="Fracture_Hanche"/>
                <Control_IfThenElse>
                  <Data_Value inValue="@Traitement"/>
                  <Control_Nothing/>
                  <Control_IfThenElse>
                    <Data_Value inValue="@Depistage"/>
                    <Control_ProcessCall inLabel="CAROC_PostFracture_Depistee"/>
                    <Control_Branch inProbability="$ref.probabilite_investigation">
                      <Control_ProcessCall inLabel="CAROC_PostFracture"/>
                      <Control_Branch inProbability="0.105">
                        <Data_SetVariable inValue_Type="Bool" inValue="true" outVariable="@Traitement"/>
                        <Control_Nothing/>
                      </Control_Branch>
                    </Control_Branch>
                  </Control_IfThenElse>
                </Control_IfThenElse>
              </Control_Sequential>
              <Control_Nothing/>
            </Meds_Event>
          </Control_IfThenElse>
          <Meds_Event inProbability="@Probabilite_Fracture_Poignet" inLabel="Fracture_Poignet">
            <Control_Sequential>
              <Control_ProcessCall inLabel="Fracture_Poignet"/>
              <Control_IfThenElse>
                <Data_Value inValue="@Traitement"/>
                <Control_Nothing/>
                <Control_IfThenElse>
                  <Data_Value inValue="@Depistage"/>
                  <Control_ProcessCall inLabel="CAROC_PostFracture_Depistee"/>
                  <Control_Branch inProbability="$ref.probabilite_investigation">
                    <Control_ProcessCall inLabel="CAROC_PostFracture"/>
                    <Control_Branch inProbability="0.105">
                      <Data_SetVariable inValue_Type="Bool" inValue="true" outVariable="@Traitement"/>
                      <Control_Nothing/>
                    </Control_Branch>
                  </Control_Branch>
                </Control_IfThenElse>
              </Control_IfThenElse>
            </Control_Sequential>
            <Control_Nothing/>
          </Meds_Event>
          <Meds_Event inProbability="@Probabilite_Fracture_Colonne" inLabel="Fracture_Colonne">
            <Control_Sequential>
              <Control_ProcessCall inLabel="Fracture_Colonne"/>
              <Control_IfThenElse>
                <Data_Value inValue="@Traitement"/>
                <Control_Nothing/>
                <Control_IfThenElse>
                  <Data_Value inValue="@Depistage"/>
                  <Control_ProcessCall inLabel="CAROC_PostFracture_Depistee"/>
                  <Control_Branch inProbability="$ref.probabilite_investigation">
                    <Control_ProcessCall inLabel="CAROC_PostFracture"/>
                    <Control_Branch inProbability="0.105">
                      <Data_SetVariable inValue_Type="Bool" inValue="true" outVariable="@Traitement"/>
                      <Control_Nothing/>
                    </Control_Branch>
                  </Control_Branch>
                </Control_IfThenElse>
              </Control_IfThenElse>
            </Control_Sequential>
            <Control_Nothing/>
          </Meds_Event>
          <Control_ProcessCall inLabel="MAJ_QALY"/>
          <Meds_Event inProbability="@Taux_Mortalite" inLabel="Deces">
            <Control_Sequential>
              <Data_SetVariable inValue_Type="Bool" inValue="true" outVariable="@Mort"/>
              <Control_ProcessPush inTarget="environment" inDelay="0" inLabel="Retirer_Individu"/>
            </Control_Sequential>
            <Control_Sequential>
              <Operators_AddVariable inArgLeft="@Duree_Vie" inArgRight="1" inArgRight_Type="Int" outResult="@Duree_Vie"/>
              <Control_ProcessPush inTarget="current" inDelay="1" inLabel="Evenements_AucunePrevention"/>
            </Control_Sequential>
          </Meds_Event>
        </Control_Sequential>
      </Control_IfThenElse>
    </PrimitiveTree>
  </Process>
